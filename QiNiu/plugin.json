{
    "unikey": "QiNiu",
    "type": 1,
    "name": "七牛云",
    "description": "Fresns 官方开发的「七牛云」存储服务插件。本插件没有设置页，七牛云配置信息使用系统的「存储设置」信息。",
    "author": "Fresns",
    "authorLink": "https://fresns.org",
    "version": "1.4.0",
    "scene": [
        "storage"
    ],
    "accessPath": "/qiniu/upload?sign={sign}&token={uploadToken}&uploadInfo={uploadInfo}&callback={uuid}&lang={langTag}",
    "settingPath": null,
    "priority": 0,
    "composer": {
        "require": {
            "overtrue/laravel-filesystem-qiniu": "^1.0"
        },
        "require-dev": []
    },
    "providers": [
        "Plugins\\QiNiu\\Providers\\QiNiuServiceProvider",
        "Plugins\\QiNiu\\Providers\\RouteServiceProvider",
        "Plugins\\QiNiu\\Providers\\EventServiceProvider",
        "Plugins\\QiNiu\\Providers\\CmdWordServiceProvider"
    ],
    "aliases": [],
    "requires": []
}